<div id="settingt2" class="tab-pane" ng-controller="adminSetting">
    <!-- <h3>Administrators</h3>
     <hr />-->
    <div class="user-txt" style="width:100%;">
        <a data-toggle="modal" data-target="#useradd-modal" href="javascript:void(0)"><img class="img-responsive user-add-icon" src="~/Images/user-add-icon.png" alt="" />Add Administrator</a>
    </div>
    <div class="table-responsive" style="float: left;width: 100%;">
        <table class="table table-bordered m-0">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Location </th>
                    <th>Department </th>
                    <th>Job Role </th>
                    <th>Length Of Service </th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody ng-repeat="emp in AdminDetails">
                <tr>
                    <td><a>{{emp.AssignedEmployeeName}}</a></td>
                    <td>{{emp.Location}}</td>
                    <td>{{emp.Department}}</td>
                    <td>{{emp.JobRole}}</td>
                    <td>{{emp.EmailID}}</td>
                    <td><a href="#">Delete</a></td>
                </tr>
            </tbody>
            @*<tbody>
                <tr>
                    <td><a class="empname-click">vinod dudala</a></td>
                    <td>Mark</td>
                    <td>Otto</td>
                    <td>mdo</td>
                    <td>mdo</td>
                </tr>
                </tbody>*@
        </table>
    </div>
    <form name="formAdmin" role="form" id="formAdmin" class="edit-form" novalidate>
        <div id="useradd-modal" class="modal fade modal-style wizard-style" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-md">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        <h4 class="modal-title" id="myModalLabel">Add Administrator</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group col-md-12">
                            <label class="col-md-12  control-label required" for="employee">Assign as Administrator</label>
                            <div class="col-md-12 ">
                                <select class="form-control" id="employee" name="employee" required ng-model="selectedemployee" ng-options="emp.text for emp in AssignedEmployees">
                                    <option value="">--- Select ---</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group col-md-12">

                            <div class="col-md-12 ">
                                <label class=" control-label" for="employee">Receive notifications?</label>
                                <label class="check-input"><input type="checkbox" id="chkNoticiation"><span class="checkmark"></span></label>
                            </div>
                        </div>
                        <div class="form-group clearfix">
                            <div class="col-md-12">
                                <div class="form-group" style="text-align:center;">
                                    <div class="input-group input-file" name="btnEmpSave">
                                        <button class="round-button waves-effect" ng-click="onSaveAdminSetting(formAdmin)" type="submit">Save</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <modal-dialog show="modalShown" message="Successfully" width="600px" height="200">
                        <div>
                            <h4>Notification</h4>
                            <br />
                            <p>{{Message}}</p>
                        </div>
                    </modal-dialog>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div>
        </form>

</div>
